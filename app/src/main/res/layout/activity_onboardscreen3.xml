<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".onboardscreen3">

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="409dp"
        android:layout_height="919dp"
        android:scaleType="centerCrop"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/group_45" />

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0">

        <TextView
            android:id="@+id/textView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Fast delivery"
            android:textColor="#000000"
            android:textSize="48sp"
            android:textStyle="bold"
            android:translationX="50dp"
            android:translationY="400dp" />

        <TextView
            android:id="@+id/textView2"
            android:layout_width="wrap_content"
            android:layout_height="156dp"
            android:text="@string/fast"
            android:textAlignment="center"
            android:textSize="34sp"
            android:textStyle="bold|italic"
            android:translationY="500dp" />

        <Button
            android:id="@+id/button4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="#B7F57F17"
            android:text="Back"
            android:translationX="30dp"
            android:translationY="680dp"
            android:contentDescription="navigate"
            />

        <ImageView
            android:id="@+id/imageView4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:translationX="150dp"
            android:translationY="700dp"
            app:srcCompat="@drawable/ellipse_4" />

        <ImageView
            android:id="@+id/imageView6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:translationX="200dp"
            android:translationY="700dp"
            app:srcCompat="@drawable/ellipse_4" />

        <ImageView
            android:id="@+id/imageView7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:translationX="250dp"
            android:translationY="700dp"
            app:srcCompat="@drawable/ellipse_3" />

        <ImageButton
            android:id="@+id/imageButton4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:translationX="310dp"
            android:translationY="680dp"
            app:srcCompat="@drawable/vector__3_"
            android:contentDescription="navi"
            />
    </FrameLayout>

</androidx.constraintlayout.widget.ConstraintLayout>